    number    = {08},
    pages     = {S08001},
    publisher = {IOP Publishing},
    title     = {{LHC Machine}},
    url       = {https://iopscience.iop.org/article/10.1088/1748-0221/3/08/S08001 https://iopscience.iop.org/article/10.1088/1748-0221/3/08/S08001/meta},
    volume    = {3},
